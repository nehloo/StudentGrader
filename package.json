{
  "name": "studentgrader",
  "private": true,
  "version": "1.0.0",
  "description": "StudentGrader",
  "repository": "",
  "license": "MIT",
  "homepage": "/studentgrader/",
  "framework7": {
    "cwd": "",
    "type": [
      "pwa",
      "web"
    ],
    "name": "StudentGrader",
    "framework": "react",
    "template": "single-view",
    "cssPreProcessor": false,
    "bundler": "webpack",
    "webpack": {
      "developmentSourceMap": true,
      "productionSourceMap": true,
      "hashAssets": false,
      "preserveAssetsPaths": false,
      "inlineAssets": true
    },
    "theming": {
      "customColor": false,
      "color": "#007aff",
      "darkTheme": false,
      "iconFonts": true,
      "fillBars": false
    },
    "customBuild": false
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js --host localhost",
    "build": "cross-env NODE_ENV=production node ./build/build.js",
    "build-dev": "cross-env NODE_ENV=development node ./build/build.js",
    "build-prod": "cross-env NODE_ENV=production node ./build/build.js",
    "postinstall": "cpy './node_modules/framework7-icons/fonts/*.*' './src/fonts/'"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "dom7": "^4.0.6",
    "file-saver": "^2.0.5",
    "file-uploader-js": "^0.6.0",
    "framework7": "^8.3.4",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "bootstrap": "^5.3.8",
    "react-dom": "^18.2.0",
    "react-indexed-db": "^2.0.1",
    "react-router-dom": "^7.8.2",
    "template7": "^1.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.28.3",
    "babel-loader": "^10.0.0",
    "chalk": "^4.1.2",
    "copy-webpack-plugin": "^13.0.1",
    "cpy": "^12.0.1",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "ora": "^8.2.0",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.3.1",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0"
  }
}
